<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.sim.traveltool">

    <application
        android:name="com.sim.wangyi.Application"
        android:allowBackup="true"
        android:icon="@mipmap/common_ic_app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/common_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/common_APPTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.sim.wangyi.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>